.team-container {
  width: 1110px;
  padding-top: 96px;
  padding-bottom: 96px;
  margin-left: auto;
  margin-right: auto;
}

.team-title {
  font-family: 'Alata';
  font-style: normal;
  font-weight: 400;
  font-size: 90px;
  line-height: 100px;
  color: #0B0D21;
}

.team-separator-stripe {
  display: block !important;
  height: 6px;
  width: 100px;
  background: #F9D023;
  margin-top: 48px;
  margin-bottom: 56px;
}

.team-content-container {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  gap: 30px;
}

